<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.doctort.HomeTamilActivity">

    <ImageView
        android:id="@+id/backgroundEnglish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/englishdashboardnew"
        tools:layout_editor_absoluteX="0dp" />

    <ImageButton
        android:id="@+id/btnSymptomTamil"
        android:layout_width="176dp"
        android:layout_height="165dp"
        android:background="@color/white"
        android:maxWidth="176dp"
        android:maxHeight="160dp"
        app:layout_constraintBottom_toTopOf="@+id/btnHelpTamil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.968"
        app:srcCompat="@drawable/symptom_btn_tamil" />

    <ImageButton
        android:id="@+id/btnRemedyTamil"

        android:layout_width="176dp"
        android:layout_height="165dp"
        android:background="@color/white"
        android:maxWidth="176dp"
        android:maxHeight="160dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnHelpTamil"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.918"
        app:srcCompat="@drawable/remedy_btn_tamil" />

    <ImageButton
        android:id="@+id/btnHelpTamil"

        android:layout_width="176dp"
        android:layout_height="165dp"
        android:background="@color/white"
        android:maxWidth="176dp"
        android:maxHeight="160dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.917"
        app:srcCompat="@drawable/help_btn_tamil" />

    <ImageButton
        android:id="@+id/btnPredictTamil"
        android:layout_width="176dp"
        android:layout_height="165dp"
        android:background="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/btnRemedyTamil"
        app:layout_constraintEnd_toStartOf="@+id/btnSymptomTamil"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.966"
        app:srcCompat="@drawable/predict_btn_tamil1" />

</androidx.constraintlayout.widget.ConstraintLayout>